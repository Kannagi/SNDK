$(OBJDIR_BIN)/src/pc.o: src/pc.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c src/pc.c -o $(OBJDIR_BIN)/src/pc.o

$(OBJDIR_BIN)/src/pce.o: src/pce.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c src/pce.c -o $(OBJDIR_BIN)/src/pce.o

$(OBJDIR_BIN)/src/snes.o: src/snes.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c src/snes.c -o $(OBJDIR_BIN)/src/snes.o

$(OBJDIR_BIN)/src/tmx.o: src/tmx.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c src/tmx.c -o $(OBJDIR_BIN)/src/tmx.o

clean_bin: 
