CFLAGS = -fomit-frame-pointer -fexpensive-optimizations -O1 -w -Wall -s
RESINC = 
LIBDIR = 
LIB = 
LDFLAGS = -s

INC_BIN = $(INC)
CFLAGS_BIN = $(CFLAGS)
RESINC_BIN = $(RESINC)
RCFLAGS_BIN = $(RCFLAGS)
LIBDIR_BIN = $(LIBDIR)
LIB_BIN = $(LIB)
LDFLAGS_BIN = $(LDFLAGS)
OBJDIR_BIN = obj
DEP_BIN = 
OUT_BIN = bin/retrotmx

OBJ_BIN = $(OBJDIR_BIN)/src/aura.o $(OBJDIR_BIN)/src/main.o $(OBJDIR_BIN)/src/md_custom.o $(OBJDIR_BIN)/src/ng.o $(OBJDIR_BIN)/src/pc.o $(OBJDIR_BIN)/src/pce.o $(OBJDIR_BIN)/src/snes.o $(OBJDIR_BIN)/src/tmx.o

all: bin bin__windows_

clean: clean_bin clean_bin__windows_

before_bin: 
	test -d bin || mkdir -p bin
	test -d $(OBJDIR_BIN)/src || mkdir -p $(OBJDIR_BIN)/src

after_bin: 

bin: before_bin out_bin after_bin

out_bin: before_bin $(OBJ_BIN) $(DEP_BIN)
	$(LD) $(LIBDIR_BIN) -o $(OUT_BIN) $(OBJ_BIN)  $(LDFLAGS_BIN) $(LIB_BIN)

$(OBJDIR_BIN)/src/aura.o: src/aura.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c src/aura.c -o $(OBJDIR_BIN)/src/aura.o

$(OBJDIR_BIN)/src/main.o: src/main.c
