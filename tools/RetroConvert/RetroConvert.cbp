<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="RetroConvert" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="bin">
				<Option output="bin/retroconvert" prefix_auto="1" extension_auto="1" />
