				<Option compiler="mingw__compiler" />
				<Option parameters="-nes mario.png" />
				<Compiler>
					<Add option="-Wextra" />
					<Add option="-Wall" />
					<Add option="-fexceptions" />
				</Compiler>
			</Target>
		</Build>
		<Compiler>
			<Add option="-fexpensive-optimizations" />
			<Add option="-O2" />
		</Compiler>
		<Linker>
			<Add option="-s" />
