$(OBJDIR_BIN)/src/palette.o: src/palette.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c src/palette.c -o $(OBJDIR_BIN)/src/palette.o

$(OBJDIR_BIN)/src/pce.o: src/pce.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c src/pce.c -o $(OBJDIR_BIN)/src/pce.o

$(OBJDIR_BIN)/src/sms.o: src/sms.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c src/sms.c -o $(OBJDIR_BIN)/src/sms.o

$(OBJDIR_BIN)/src/snes.o: src/snes.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c src/snes.c -o $(OBJDIR_BIN)/src/snes.o

$(OBJDIR_BIN)/src/tilemap.o: src/tilemap.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c src/tilemap.c -o $(OBJDIR_BIN)/src/tilemap.o

clean_bin: 
	rm -f $(OBJ_BIN) $(OUT_BIN)
	rm -rf bin
	rm -rf $(OBJDIR_BIN)/src

.PHONY: before_bin after_bin clean_bin

