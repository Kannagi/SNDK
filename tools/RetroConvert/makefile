CC = gcc
CXX = g++
AR = ar
LD = g++
WINDRES = windres

INC = 
CFLAGS = -fexpensive-optimizations -O2
RESINC = 
LIBDIR = 
LIB = -lSDL -lSDL_image
LDFLAGS = -s -static-libgcc

INC_BIN = $(INC)
CFLAGS_BIN = $(CFLAGS) -w -Wextra -Wall -fexceptions
RESINC_BIN = $(RESINC)
RCFLAGS_BIN = $(RCFLAGS)
LIBDIR_BIN = $(LIBDIR)
LIB_BIN = $(LIB)
LDFLAGS_BIN = $(LDFLAGS) -lm
OBJDIR_BIN = obj
DEP_BIN = 
OUT_BIN = bin/retroconvert

OBJ_BIN = $(OBJDIR_BIN)/src/main.o $(OBJDIR_BIN)/src/md.o $(OBJDIR_BIN)/src/nes.o $(OBJDIR_BIN)/src/ng.o $(OBJDIR_BIN)/src/palette.o $(OBJDIR_BIN)/src/pce.o $(OBJDIR_BIN)/src/sms.o $(OBJDIR_BIN)/src/snes.o $(OBJDIR_BIN)/src/tilemap.o

all: bin bin__windows_

clean: clean_bin clean_bin__windows_

before_bin: 
	test -d bin || mkdir -p bin
	test -d $(OBJDIR_BIN)/src || mkdir -p $(OBJDIR_BIN)/src

after_bin: 

bin: before_bin out_bin after_bin

out_bin: before_bin $(OBJ_BIN) $(DEP_BIN)
	$(LD) $(LIBDIR_BIN) -o $(OUT_BIN) $(OBJ_BIN)  $(LDFLAGS_BIN) $(LIB_BIN)

$(OBJDIR_BIN)/src/main.o: src/main.c
	$(CC) $(CFLAGS_BIN) $(INC_BIN) -c src/main.c -o $(OBJDIR_BIN)/src/main.o

